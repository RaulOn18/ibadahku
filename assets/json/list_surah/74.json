      "ayah": "9",
      "hizb": "58.25",
      "id": "5504",
      "juz": "29",
      "latin": "Fa żālika yauma'iżiy yaumun ‘asīr(un).",
      "notes": null,
      "page": "575",
      "surah": "74",
      "text": "hari itulah hari yang sulit,",
      "theme": "1105"
    },
    {
