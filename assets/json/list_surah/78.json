      "audio": "https://cdn.alquran.cloud/media/audio/ayah/ar.alafasy/5676",
      "ayah": "4",
      "hizb": "59",
      "id": "5676",
      "juz": "30",
      "latin": "Kallā saya‘lamūn(a).",
      "notes": null,
      "page": "582",
      "surah": "78",
      "text": "Sekali-kali tidak! Kelak mereka akan mengetahui.",
      "theme": ""
    },
    {
      "arab": "ثُمَّ كَلَّا سَيَعْلَمُوْنَ",
      "asbab": "0",
